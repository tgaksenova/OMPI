<Window x:Class="_1pract.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_1pract"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>

        <Grid.RowDefinitions>
            <RowDefinition Height="42"/>
            <RowDefinition/>
            <RowDefinition Height="42"/>
        </Grid.RowDefinitions>
        <Rectangle Fill="#FFF4F4F5" Grid.Row="3" StrokeThickness="0"/>
        <Button Content="Назад" x:Name="ButtonBack" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="10,12,0,0" Click="ButtonBack_Click"/>
        <Frame x:Name="MainFrame" Source="Pages/AuthPage.xaml" Navigated="MainFrame_OnNavigated" HorizontalAlignment="Left" Height="315" VerticalAlignment="Top" Width="753" Margin="10,10,0,0" Grid.Row="1"/>
    </Grid>


</Window>
